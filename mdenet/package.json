{
  "name": "mdenet",
  "displayName": "MDENet",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "activities.refresh",
        "title": "Refresh Activities"
      },
      {
        "command": "tasks.refresh",
        "title": "Refresh Tasks"
      },
      {
        "command": "panels.refresh",
        "title": "Refresh Panels"
      },
      {
        "command": "activities.play",
        "title": "Play Activity"
      },
      {
        "command": "activities.stop",
        "title": "Stop Activity"
      },
      {
        "command": "panels.displayPanel",
        "title": "Display Panel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mdenetActivity",
          "title": "MDENet",
          "icon": "resources/M.svg"
        }
      ]
    },
    "views":{
      "mdenetActivity": [
        {
          "id": "activities",
          "name": "Activities"
        },
        {
          "id": "tasks",
          "name": "Tasks"
        },
        {
          "id": "panels",
          "name": "Panels"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
